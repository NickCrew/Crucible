{
  "name": "demo-dashboard",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/demo-dashboard/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc",
        "cwd": "apps/demo-dashboard"
      },
      "dependsOn": ["^build"]
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "apps/demo-dashboard"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsx watch src/server/index.ts",
        "cwd": "apps/demo-dashboard"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run",
        "cwd": "apps/demo-dashboard"
      }
    }
  }
}
