  "steps": [
    {
      "id": "healthcare-recon-1",
      "name": "Healthcare System Discovery",
      "stage": "reconnaissance",
      "request": {
        "method": "GET",
        "url": "/api/hipaa/directory",
        "headers": {
          "User-Agent": "Mozilla/5.0 (compatible; Medical-Research-Bot/1.0)"
        }
      },
      "execution": {
        "delayMs": 1500
      }
    },
    {
      "id": "healthcare-recon-2",
      "name": "Provider Network Enumeration",
      "stage": "reconnaissance",
      "request": {
        "method": "GET",
        "url": "/api/providers/network/search",
        "headers": {
          "User-Agent": "HealthInsuranceBot/2.1 (+http://healthfinder.gov/)",
          "Accept": "application/json"
        },
        "params": {
          "specialty": "all",
          "location": "nationwide",
          "insurance_accepted": "all"
        }
      },
      "execution": {
        "delayMs": 2000
      },
      "dependsOn": [
        "healthcare-recon-1"
      ]
    },
    {
      "id": "healthcare-phi-probe-1",
      "name": "PHI Access Point Discovery",
      "stage": "reconnaissance",
      "request": {
        "method": "GET",
        "url": "/api/medical/phi/endpoints",
        "headers": {
          "User-Agent": "Healthcare-Integration-Service/3.2",
          "Accept": "application/json"
        }
      },
      "execution": {
        "delayMs": 2500
      },
      "dependsOn": [
        "healthcare-recon-2"
      ]
    },
    {
      "id": "healthcare-auth-bypass-1",
      "name": "Provider Authentication Bypass Attempt",
      "stage": "delivery",
      "request": {
        "method": "POST",
        "url": "/api/providers/auth/login",
        "headers": {
          "Content-Type": "application/json",
          "User-Agent": "ProviderPortal/4.1.2"
        },
        "body": "{\"provider_id\": \"NPI1234567890\", \"credentials\": \"bypass\", \"auth_type\": \"emergency_access\", \"reason\": \"patient_critical_care\"}"
      },
      "execution": {
        "delayMs": 1000
      },
      "expect": {
        "blocked": true
      },
      "dependsOn": [
        "healthcare-phi-probe-1"
      ]
    },
    {
      "id": "healthcare-records-access-1",
      "name": "Unauthorized Medical Records Access",
      "stage": "exploitation",
      "request": {
        "method": "GET",
        "url": "/api/hipaa/records/patient",
        "headers": {
          "User-Agent": "Medical-Records-System/5.0",
          "Authorization": "Bearer fake_provider_token",
          "X-Emergency-Access": "true"
        },
        "params": {
          "patient_id": "all",
          "date_range": "lifetime",
          "include_sensitive": "true",
          "export_format": "complete_medical_history"
        }
      },
      "execution": {
        "delayMs": 3000
      },
      "expect": {
        "blocked": true
      },
      "dependsOn": [
        "healthcare-auth-bypass-1"
      ]
    },
    {
      "id": "healthcare-insurance-exploit-1",
      "name": "Insurance Portability Data Manipulation",
      "stage": "exploitation",
      "request": {
        "method": "POST",
        "url": "/api/insurance/portability/transfer",
        "headers": {
          "Content-Type": "application/json",
          "Authorization": "Bearer compromised_insurance_token"
        },
        "body": "{\"source_policy\": \"POL123456789\", \"destination_policy\": \"attacker_controlled\", \"transfer_all_benefits\": true, \"bypass_waiting_period\": true, \"pre_existing_conditions\": \"clear\"}"
      },
      "execution": {
        "delayMs": 4000
      },
      "expect": {
        "blocked": true
      },
      "dependsOn": [
        "healthcare-records-access-1"
      ]
    },
    {
      "id": "healthcare-provider-abuse-1",
      "name": "Provider Network Resource Abuse",
      "stage": "exploitation",
      "request": {
        "method": "PUT",
        "url": "/api/providers/network/billing",
        "headers": {
          "Content-Type": "application/json",
          "X-Provider-ID": "NPI9999999999"
        },
        "body": "{\"procedure_codes\": [\"99999\", \"EXPERIMENTAL\"], \"billing_multiplier\": 10.0, \"insurance_override\": \"approve_all\", \"claim_amount\": 999999.99}"
      },
      "execution": {
        "delayMs": 3500
      },
      "expect": {
        "blocked": true
