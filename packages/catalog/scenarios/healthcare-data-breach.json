    {
      "id": "healthcare-persistence-1",
      "name": "Backdoor in Medical Records System",
      "stage": "persistence",
      "request": {
        "method": "PUT",
        "url": "/api/hipaa/system/configuration",
        "headers": {
          "Content-Type": "application/json",
          "Authorization": "Bearer system_admin_medical_token"
        },
        "body": "{\"audit_logging\": \"disabled\", \"access_controls\": \"bypass_for_emergency\", \"backup_system\": \"external_attacker_controlled\", \"encryption\": \"weakened_for_performance\"}"
      },
      "execution": {
        "delayMs": 8500
      },
      "expect": {
        "blocked": true
      },
      "dependsOn": [
        "healthcare-exfiltration-1"
      ]
    },
    {
      "id": "healthcare-cleanup-1",
      "name": "Medical Audit Log Manipulation",
      "stage": "defense-evasion",
      "request": {
        "method": "DELETE",
        "url": "/api/hipaa/audit-logs",
        "headers": {
          "User-Agent": "Medical-System-Maintenance/2.1",
          "Authorization": "Bearer maintenance_token"
        },
        "params": {
          "time_range": "last_24_hours",
          "event_types": "data_access,phi_export,authentication_bypass,system_configuration",
          "user_activities": "suspicious_patterns"
