        "method": "POST",
        "url": "/api/medical/prescriptions/create",
        "headers": {
          "Content-Type": "application/json",
          "Authorization": "Bearer doctor_token_stolen"
        },
        "body": "{\"patient_id\": \"target_patient\", \"medication\": \"controlled_substances\", \"quantity\": 9999, \"refills\": 99, \"prescriber_override\": true, \"dea_number\": \"fake_dea_123\"}"
      },
      "execution": {
        "delayMs": 4500
      },
      "expect": {
        "blocked": true
      },
      "dependsOn": [
        "healthcare-provider-abuse-1"
      ]
    },
    {
      "id": "healthcare-hipaa-violation-1",
      "name": "Mass HIPAA Data Extraction",
      "stage": "collection",
      "request": {
        "method": "GET",
        "url": "/api/hipaa/export/bulk",
        "headers": {
          "User-Agent": "HIPAA-Compliance-Audit-Tool/1.0",
          "Authorization": "Bearer escalated_admin_token"
        },
        "params": {
          "patients": "all_active",
          "data_types": "demographics,medical_history,prescriptions,insurance,lab_results,imaging",
          "format": "unencrypted_csv",
          "destination": "external_server"
        }
      },
      "execution": {
        "delayMs": 8000
      },
      "expect": {
        "blocked": true
      },
      "dependsOn": [
        "healthcare-prescription-fraud-1"
