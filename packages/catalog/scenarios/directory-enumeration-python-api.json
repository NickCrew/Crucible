{
  "name": "Directory Enumeration (Python/Flask API)",
  "description": "Slow directory and file discovery targeting Python/Flask API paths over 10-15 minutes",
  "category": "Reconnaissance",
  "difficulty": "Beginner",
  "tags": [
    "directory-enum",
    "recon",
    "low-risk",
    "discovery",
