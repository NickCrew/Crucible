{
  "name": "Directory Enumeration",
  "description": "Slow directory and file discovery probing common paths over 10-15 minutes",
  "category": "Reconnaissance",
  "difficulty": "Beginner",
  "tags": [
    "directory-enum",
    "recon",
    "low-risk",
    "discovery"
  ],
  "estimatedDuration": "10-15 minutes",
  "id": "directory-enumeration-slow",
  "steps": [
    {
      "id": "step-1",
      "name": "Check robots.txt for crawl restrictions",
      "stage": "reconnaissance",
      "request": {
        "method": "GET",
        "url": "/robots.txt"
      }
    },
    {
      "id": "step-2",
      "name": "Enumerate sitemap for site structure",
      "stage": "reconnaissance",
      "request": {
        "method": "GET",
        "url": "/sitemap.xml"
      }
    },
    {
      "id": "step-3",
      "name": "Check for security policy disclosure",
      "stage": "reconnaissance",
      "request": {
        "method": "GET",
        "url": "/.well-known/security.txt"
      }
    },
    {
      "id": "step-4",
      "name": "Probe for admin interface",
      "stage": "reconnaissance",
      "request": {
        "method": "GET",
        "url": "/admin"
      }
    },
    {
      "id": "step-5",
      "name": "Check alternative admin path",
      "stage": "reconnaissance",
      "request": {
        "method": "GET",
        "url": "/administrator"
