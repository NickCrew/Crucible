{
  "name": "catalog",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/catalog/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc",
        "cwd": "packages/catalog"
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "packages/catalog"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run",
        "cwd": "packages/catalog"
      }
    }
  }
}
